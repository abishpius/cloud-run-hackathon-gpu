# Healthcare Assistant Frontend - Environment Variables
# Copy this file to .env and update with your values

# Backend API URL
# For local development: http://localhost:8080
# For Cloud Run: https://your-backend-service-url
BACKEND_API_URL=http://localhost:8080

# Authentication
COOKIE_KEY=your_secret_cookie_key_change_in_production

# User database file
USERS_FILE=config/users.yaml

# Optional: Enable streaming responses
ENABLE_STREAMING=false

# Streamlit server port
PORT=8080
